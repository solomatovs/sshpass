{
    // Используйте IntelliSense, чтобы узнать о возможных атрибутах.
    // Наведите указатель мыши, чтобы просмотреть описания существующих атрибутов.
    // Для получения дополнительной информации посетите: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "sshpass_ubuntu_1",
            "cargo": {
                "args": [
                    "build",
                    "--bin=sshpass",
                    "--package=sshpass"
                ],
                "filter": {
                    "name": "sshpass",
                    "kind": "bin"
                }
            },
            "args": [
                "ssh",
                "ubuntu_1"
            ],
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.vscode/.env",
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "sshpass_bash",
            "cargo": {
                "args": [
                    "build",
                    "--bin=sshpass",
                    "--package=sshpass"
                ],
                "filter": {
                    "name": "sshpass",
                    "kind": "bin"
                }
            },
            "args": [
                "bash",
            ],
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.vscode/.env",
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "sshpass_muduck",
            "cargo": {
                "args": [
                    "build",
                    "--bin=sshpass",
                    "--package=sshpass"
                ],
                "filter": {
                    "name": "sshpass",
                    "kind": "bin"
                }
            },
            "args": [
                "ssh",
                "muduck.com",
                "-p",
                "475"
            ],
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.vscode/.env",
        },
    ]
}